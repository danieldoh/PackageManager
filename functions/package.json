{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "fix": "./node_modules/.bin/eslint --fix",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions,firestore,storage",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "kill": "npx kill-port 5000 5001 8080 8085 4000 9229",
    "webpack": "node_modules/.bin/webpack",
    "dev": "npx nodemon -e ts --exec 'npx ts-node ./src/index.ts'",
    "test": "jest --watchAll"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/semver": "^7.3.13",
    "@types/winston": "^2.4.4",
    "blob-util": "^2.0.2",
    "cors": "^2.8.5",
    "cross-blob": "^3.0.2",
    "express": "^4.18.2",
    "firebase": "^9.21.0",
    "firebase-admin": "^11.7.0",
    "firebase-functions": "^4.2.0",
    "firebaseui": "^6.0.2",
    "fs": "^0.0.1-security",
    "node-fetch": "^2.0.0",
    "winston": "^3.8.2",
    "xhr2": "^0.2.1"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/node-fetch": "^2.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "style-loader": "^3.3.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.0"
  },
  "private": true,
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
